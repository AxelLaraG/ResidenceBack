<?xml version="1.0"?>
<rdf:RDF
    xmlns="http://www.semanticweb.org/ontologies/2024/PRODEP#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xml="http://www.w3.org/XML/1998/namespace"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:prodep="http://www.semanticweb.org/ontologies/2024/PRODEP#">
    
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP">
        <rdfs:comment>Ontología para representar el esquema PRODEP basado en el XSD proporcionado</rdfs:comment>
    </owl:Ontology>

    <!-- ############################## -->
    <!-- ### CLASES PRINCIPALES ### -->
    <!-- ############################## -->
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#CVU">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Elemento raíz que representa el Curriculum Vitae Único del programa PRODEP</rdfs:comment>
    </owl:Class>
    
    <!-- Identificación del académico -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#Identificacion">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Datos personales y de identificación del académico</rdfs:comment>
    </owl:Class>
    
    <!-- Formación académica -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#EstudiosRealizados">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Historial de estudios formales del académico (licenciatura, maestría, doctorado)</rdfs:comment>
    </owl:Class>
    
    <!-- Experiencia laboral -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#DatosLaborales">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Información sobre los puestos de trabajo y nombramientos del académico</rdfs:comment>
    </owl:Class>
    
    <!-- Área de especialización -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#AreaDisciplinaActual">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Área y disciplina principal de investigación o trabajo del académico</rdfs:comment>
    </owl:Class>
    
    <!-- Producción académica -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#Produccion">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Producción académica incluyendo artículos, libros, capítulos, memorias, etc.</rdfs:comment>
    </owl:Class>
    
    <!-- Actividades docentes -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#Docencia">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Cursos impartidos por el académico</rdfs:comment>
    </owl:Class>
    
    <!-- Tutorías realizadas -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#Tutorias">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Tutorías académicas proporcionadas a estudiantes</rdfs:comment>
    </owl:Class>
    
    <!-- Dirección de tesis/proyectos -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#DireccionIndividualizada">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Dirección de tesis, proyectos de investigación o trabajos terminales</rdfs:comment>
    </owl:Class>
    
    <!-- Gestión académica -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#GestionAcademica">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Actividades de gestión o administración académica realizadas</rdfs:comment>
    </owl:Class>
    
    <!-- Líneas de investigación -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#LineaGAC">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Líneas de Generación y Aplicación del Conocimiento (LGAC) en las que participa</rdfs:comment>
    </owl:Class>
    
    <!-- Beneficios externos -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#BeneficiosExternos">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Beneficios o apoyos recibidos de fuentes externas a la institución</rdfs:comment>
    </owl:Class>
    
    <!-- Proyectos de investigación -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#ProyectosDeInvestigacion">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Proyectos de investigación en los que ha participado</rdfs:comment>
    </owl:Class>
    
    <!-- Beneficios PROMEP/PRODEP -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#BeneficiosPROMEP">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Beneficios recibidos del programa PROMEP/PRODEP</rdfs:comment>
    </owl:Class>
    
    <!-- Cuerpo académico -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#CuerpoAcademico">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Información sobre el cuerpo académico al que pertenece</rdfs:comment>
    </owl:Class>
    
    <!-- Programas educativos -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#ProgramasEducativos">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>Programas educativos en los que participa</rdfs:comment>
    </owl:Class>

    <!-- ############################## -->
    <!-- ### SUBCLASES ESPECÍFICAS ### -->
    <!-- ############################## -->
    
    <!-- Tipo de estudio -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#Estudio">
