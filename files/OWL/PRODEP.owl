        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2024/PRODEP#CVU"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2024/PRODEP#Identificacion"/>
        <rdfs:comment>Relaciona el CVU con los datos de identificación del académico</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2024/PRODEP#tieneEstudios">
