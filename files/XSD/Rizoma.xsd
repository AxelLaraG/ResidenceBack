	<!--Trabajos
	de Titulación-->
	<xs:complexType name="TrabajosDeTitulacionType">
		<xs:choice>
			<xs:element name="TrabajoSNP" type="TrabajoSNPType" minOccurs="0" maxOccurs="unbounded" />
