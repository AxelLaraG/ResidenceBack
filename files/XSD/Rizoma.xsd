			<xs:element name="CitaLibro" minOccurs="0" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="URLCita" type="xs:string" />
						<xs:element name="CitaA" type="xs:integer" />
						<xs:element name="CitaB" type="xs:integer" />
						<xs:element name="TotalDeCitas" type="xs:integer" />
						<xs:element name="EjeSECIHTI" type="xs:string" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!--Autores-->
	<xs:complexType name="AutoresType">
		<xs:sequence>
			<xs:element name="Autor" type="AutorType" minOccurs="1" maxOccurs="unbounded" />
