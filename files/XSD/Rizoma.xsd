	<xs:complexType name="GEMAType">
		<xs:sequence>
			<xs:element name="URL" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<!--SISDAI-->
	<xs:complexType name="SisdaiType">
		<xs:sequence>
			<xs:element name="Bibliotecas" type="xs:string" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Descripcion" type="Max2000Caracteres" />
		</xs:sequence>
	</xs:complexType>

	<!--DATOS
	REUTILIZABLES-->

	<!--Máximo
	2000 caracteres-->
	<xs:simpleType name="Max2000Caracteres">
		<xs:restriction base="xs:string">
			<xs:maxLength value="2000" />
		</xs:restriction>
	</xs:simpleType>

	<!--Mínimo
	y máximo 10 dígitos-->
	<xs:simpleType name="DiezDigitos">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{10}" />
		</xs:restriction>
	</xs:simpleType>

	<!--Mínimo
	y Máximo 18 caracteres-->
	<xs:simpleType name="MinMax18Caracteres">
		<xs:restriction base="xs:string">
			<xs:length value="18" />
		</xs:restriction>
	</xs:simpleType>

	<!--Mínimo
	y Máximo 13 caracteres-->
	<xs:simpleType name="MinMax13Caracteres">
		<xs:restriction base="xs:string">
			<xs:length value="13" />
		</xs:restriction>
	</xs:simpleType>

	<!--Fecha
	AAAA-MM-DD-->
	<xs:simpleType name="DateType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}-\d{2}-\d{2}" />
		</xs:restriction>
	</xs:simpleType>

	<!--Sexo
	con 2 opciones-->
	<xs:simpleType name="Sexo2Options">
		<xs:restriction base="xs:string">
			<xs:enumeration value="masculino" />
			<xs:enumeration value="femenino" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Sexo con 4 opciones-->
	<xs:simpleType name="Sexo4Options">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Masculino" />
			<xs:enumeration value="Femenino" />
			<xs:enumeration value="No binario" />
			<xs:enumeration value="Otro" />
		</xs:restriction>
	</xs:simpleType>

	<!--5
	dígitos-->
	<xs:simpleType name="MinMax5Digitos">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{1,5}" />
		</xs:restriction>
	</xs:simpleType>

	<!--Minimo
	5 Máximo 10 dígitos-->
	<xs:simpleType name="Min5Max10Digitos">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{5,10}" />
		</xs:restriction>
	</xs:simpleType>

	<!--Máximo
	500 caracteres-->
	<xs:simpleType name="Max500Caracteres">
		<xs:restriction base="xs:string">
			<xs:maxLength value="500" />
		</xs:restriction>
	</xs:simpleType>

	<!--Mínimo
	2 caracteres sin Máximo-->
	<xs:simpleType name="Min2Caracteres">
		<xs:restriction base="xs:string">
			<xs:minLength value="2" />
		</xs:restriction>
	</xs:simpleType>

	<!--Menor
	a N-->
	<xs:simpleType name="AnioType">
		<xs:restriction base="xs:gYear">
			<xs:maxInclusive value="2025" />
		</xs:restriction>
	</xs:simpleType>

	<!--Entre
	0 y 9999-->
	<xs:simpleType name="Horas4DigitosType">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="9999" />
		</xs:restriction>
	</xs:simpleType>

	<!--Máximo
	50 caracteres-->
	<xs:simpleType name="Folio50Type">
		<xs:restriction base="xs:string">
			<xs:maxLength value="50" />
		</xs:restriction>
	</xs:simpleType>

