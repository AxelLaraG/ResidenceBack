<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<!--Elemento
	Raíz-->
	<xs:element name="cvu">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MiPerfil">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Nombre" type="xs:string" />

							<xs:element name="Semblanza" minOccurs="1" maxOccurs="1"
								type="SemblanzaType" />

							<xs:element name="Contacto" type="ContactoType" minOccurs="1"
								maxOccurs="1" />

							<xs:element name="Habilidades" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Habilidad" type="HabilidadType"
											minOccurs="0" maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="Intereses" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Interes" type="xs:string" minOccurs="0"
											maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="AreaDeConocimiento" minOccurs="1" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Area" type="xs:string" minOccurs="1"
											maxOccurs="1" />
										<xs:element name="Campo" type="xs:string" minOccurs="1"
											maxOccurs="1" />
										<xs:element name="Disciplina" type="xs:string" minOccurs="1"
											maxOccurs="1" />
										<xs:element name="Subdisciplina" type="xs:string"
											minOccurs="1" maxOccurs="1" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="IdentificadoresDeAutor" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Identificador" type="IdentificadorType"
											minOccurs="0" maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
								<xs:unique name="uniqueOrganizacion">
									<xs:selector xpath="Identificador" />
									<xs:field xpath="@organizacion" />
								</xs:unique>
							</xs:element>

							<xs:element name="InformacionGeneral">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="CURP" type="CURPType" minOccurs="1"
											maxOccurs="1" />
										<xs:element name="Nombre" type="xs:string" minOccurs="1"
											maxOccurs="1" />
										<xs:element name="PrimerApellido" type="xs:string"
											minOccurs="1" maxOccurs="1" />
										<xs:element name="SegundoApellido" type="xs:string"
											minOccurs="1" maxOccurs="1" />
										<xs:element name="FechaDeNacimiento" type="DateType"
											minOccurs="1" maxOccurs="1" />
										<xs:element name="Sexo" type="SexoType" minOccurs="1"
											maxOccurs="1" />
										<xs:element name="PaisDeNacimiento" type="xs:string"
											minOccurs="1" maxOccurs="1" />
										<xs:element name="EntidadFederativa" type="xs:string"
											minOccurs="1" maxOccurs="1" />
										<xs:element name="RFC" type="RFCType" minOccurs="1"
											maxOccurs="1" />
										<xs:element name="EstadoCivil" type="EstadoCivilType"
											minOccurs="1" maxOccurs="1" />
										<xs:element name="Nacionalidad" type="xs:string"
											minOccurs="1" maxOccurs="1" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="Domicilio" minOccurs="1" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:choice>
											<!-- Contenido para México -->
											<xs:element name="Mexico">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="CodigoPostal"
															type="CodigoPostalType" minOccurs="1"
															maxOccurs="1" />
														<xs:element name="Estado" type="xs:string"
															minOccurs="1" maxOccurs="1" />
														<xs:element name="MunicipioAlcaldia"
															type="xs:string" minOccurs="1"
															maxOccurs="1" />
														<xs:element name="Localidad"
															type="xs:string" minOccurs="1"
															maxOccurs="1" />
														<xs:element name="Asentamiento"
															type="xs:string" minOccurs="0"
															maxOccurs="1" />
														<xs:element name="TipoAsentamiento"
															type="xs:string" minOccurs="0"
															maxOccurs="1" />
														<xs:element name="Tipo"
															type="TipoDomicilioType" minOccurs="1"
															maxOccurs="1" />
														<!-- Referencias y Descripcion se agregan
														opcionalmente solo si
														InformacionExtra=Verdadero -->
														<xs:choice minOccurs="0" maxOccurs="1">
															<xs:sequence>
																<xs:element name="Referencias"
																	minOccurs="0" maxOccurs="3">
																	<xs:complexType>
																		<xs:attribute
																			name="TipoDeCalle"
																			type="xs:string"
																			use="optional" />
																		<xs:attribute
																			name="NombreDeLaCalle"
																			type="xs:string"
																			use="optional" />
																	</xs:complexType>
																</xs:element>
																<xs:element name="Descripcion"
																	type="Descripcion500Type"
																	minOccurs="0" maxOccurs="1" />
															</xs:sequence>
														</xs:choice>
														<!-- Elección entre Urbano y Rural -->
														<xs:choice minOccurs="1" maxOccurs="1">
															<xs:element name="Urbano" minOccurs="0"
																maxOccurs="1">
																<xs:complexType>
																	<xs:sequence>
																		<xs:element
																			name="TipoDeVialidad"
																			type="xs:string"
																			minOccurs="1"
																			maxOccurs="1" />
																		<xs:element
																			name="NombreDeLaVialidad"
																			type="xs:string"
																			minOccurs="1"
																			maxOccurs="1" />
																		<xs:element
																			name="NumeroExterior"
																			type="NumeroExteriorType"
																			minOccurs="0"
																			maxOccurs="1" />
																		<xs:element
																			name="NumeroInterior"
																			type="NumeroInteriorType"
																			minOccurs="0"
																			maxOccurs="1" />
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
															<xs:element name="Rural" minOccurs="0"
																maxOccurs="1">
																<xs:complexType>
																	<xs:sequence>
																		<xs:element
																			name="TipoDeVialidad"
																			type="xs:string"
																			minOccurs="1"
																			maxOccurs="1" />
																		<xs:element
																			name="NombreDeLaVialidad"
																			type="xs:string"
																			minOccurs="1"
																			maxOccurs="1" />
																		<xs:element
																			name="Administracion"
																			type="xs:string"
																			minOccurs="0"
																			maxOccurs="1" />
																		<xs:element
																			name="DerechoDeTransito"
																			minOccurs="0"
																			maxOccurs="1">
																			<xs:simpleType>
																				<xs:restriction
																					base="xs:string">
																					<xs:enumeration
																						value="CUOTA" />
																					<xs:enumeration
																						value="LIBRE" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																		<xs:element name="Codigo"
																			type="xs:string"
																			minOccurs="0"
																			maxOccurs="1" />
																		<xs:element
																			name="OrigenTramo"
																			type="xs:string"
																			minOccurs="0"
																			maxOccurs="1" />
																		<xs:element
																			name="DestinoTramo"
																			type="xs:string"
																			minOccurs="0"
																			maxOccurs="1" />
																		<xs:element name="Margen"
																			minOccurs="0"
																			maxOccurs="1">
																			<xs:simpleType>
																				<xs:restriction
																					base="xs:string">
																					<xs:enumeration
																						value="DERECHO" />
																					<xs:enumeration
																						value="IZQUIERDO" />
																				</xs:restriction>
																			</xs:simpleType>
																		</xs:element>
																		<xs:element
																			name="Cadenamiento"
																			type="xs:string"
																			minOccurs="0"
																			maxOccurs="1" />
																		<xs:element
																			name="NumeroExterior"
																			type="NumeroExteriorType"
																			minOccurs="0"
																			maxOccurs="1" />
																		<xs:element
																			name="NumeroInterior"
																			type="NumeroInteriorType"
																			minOccurs="0"
																			maxOccurs="1" />
																	</xs:sequence>
																</xs:complexType>
															</xs:element>
														</xs:choice>
													</xs:sequence>
													<xs:attribute name="InformacionExtra"
														use="optional">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration value="Verdadero" />
																<xs:enumeration value="Falso" />
															</xs:restriction>
														</xs:simpleType>
													</xs:attribute>
												</xs:complexType>
											</xs:element>
											<!-- Contenido para Extranjero -->
											<xs:element name="Extranjero">
												<xs:complexType>
													<xs:sequence>
														<xs:element name="Pais" type="xs:string"
															minOccurs="1" maxOccurs="1" />
														<xs:element name="CodigoPostal"
															type="xs:string" minOccurs="1"
															maxOccurs="1" />
														<xs:element name="Estado" type="xs:string"
															minOccurs="1" maxOccurs="1" />
														<xs:element name="Ciudad" type="xs:string"
															minOccurs="1" maxOccurs="1" />
														<xs:element name="Condado" type="xs:string"
															minOccurs="1" maxOccurs="1" />
														<xs:element name="Calle" type="xs:string"
															minOccurs="1" maxOccurs="1" />
														<xs:element name="Numero" type="xs:string"
															minOccurs="1" maxOccurs="1" />
														<xs:element name="Descripcion"
															type="Descripcion500Type" minOccurs="1"
															maxOccurs="1" />
													</xs:sequence>
												</xs:complexType>
											</xs:element>
										</xs:choice>
									</xs:sequence>
									<xs:attribute name="Ubicacion" use="required">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="México" />
												<xs:enumeration value="Extranjero" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>
							</xs:element>

							<xs:element name="ContactosDeEmergencia" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Contacto" minOccurs="0"
											maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Nombre" type="NombreMin2Type" />
													<xs:element name="PrimerApellido"
														type="NombreMin2Type" />
													<xs:element name="SegundoApellido"
														type="NombreMin2Type" />
													<xs:element name="Parentesco" type="xs:string" />
													<xs:element name="FechaDeNacimiento"
														type="DateType" />
													<xs:element name="Sexo" type="Sexo2Type" />
													<xs:element name="CorreoElectrónico"
														type="CorreoContactoType" />
													<xs:element name="Lada"
														type="Horas10DigitosType" />
													<xs:element name="Telefono"
														type="Horas10DigitosType" />
													<xs:element name="Movil"
														type="Horas10DigitosType" />
													<xs:element name="MedioPreferente">
														<xs:simpleType>
															<xs:restriction base="xs:string">
																<xs:enumeration
																	value="Correo electrónico" />
																<xs:enumeration value="Teléfono" />
																<xs:enumeration value="Movil" />
															</xs:restriction>
														</xs:simpleType>
													</xs:element>
													<xs:choice>
														<xs:element name="Mexico">
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="CodigoPostal"
																		type="CodigoPostalType" />
																	<xs:element name="Estado"
																		type="xs:string" />
																	<xs:element
																		name="MunicipioAlcaldia"
																		type="xs:string" />
																	<xs:element
																		name="TipoAsentamiento"
																		type="xs:string" />
																</xs:sequence>
															</xs:complexType>
														</xs:element>
														<xs:element name="Extranjero">
															<xs:complexType>
																<xs:sequence>
																	<xs:element name="Pais"
																		type="xs:string" />
																	<xs:element name="CodigoPostal"
																		type="CPMin5Max10Type" />
																	<xs:element name="Estado"
																		type="xs:string" />
																	<xs:element name="Ciudad"
																		type="xs:string" />
																	<xs:element name="Condado"
																		type="xs:string" />
																	<xs:element name="Calle"
																		type="xs:string" />
																	<xs:element name="Numero"
																		type="xs:string" />
																	<xs:element name="Referencias">
																		<xs:simpleType>
																			<xs:restriction
																				base="xs:string">
																				<xs:maxLength
																					value="256" />
																			</xs:restriction>
																		</xs:simpleType>
																	</xs:element>
																</xs:sequence>
															</xs:complexType>
														</xs:element>
													</xs:choice>
												</xs:sequence>
												<xs:attribute name="Ubicacion" use="required">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Mexico" />
															<xs:enumeration value="Extranjero" />
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>


							<xs:element name="DependientesEconomicos" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Dependiente" minOccurs="0"
											maxOccurs="unbounded">
											<xs:complexType>
												<xs:choice>
													<xs:element name="Mexican">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="CURP"
																	type="CURPType" />
																<xs:element name="Nombre"
																	type="xs:string" />
																<xs:element name="PrimerApellido"
																	type="xs:string" />
																<xs:element name="SegundoApellido"
																	type="xs:string" />
																<xs:element name="FechaDeNacimiento"
																	type="DateType" />
																<xs:element name="Edad"
																	type="xs:positiveInteger" />
																<xs:element name="Parentesco">
																	<xs:simpleType>
																		<xs:restriction
																			base="xs:string">
																			<xs:enumeration
																				value="Concubino" />
																			<xs:enumeration
																				value="Cónyugue" />
																			<xs:enumeration
																				value="Hija" />
																			<xs:enumeration
																				value="Hija mayor de edad" />
																			<xs:enumeration
																				value="Hijo" />
																			<xs:enumeration
																				value="Hijo mayor de edad" />
																			<xs:enumeration
																				value="Madre" />
																			<xs:enumeration
																				value="Padre" />
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="Sexo"
																	type="Sexo2Type" />
																<xs:element name="Orden">
																	<xs:simpleType>
																		<xs:restriction
																			base="xs:integer">
																			<xs:maxInclusive
																				value="999" />
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>

													<xs:element name="Foreign">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Nombre"
																	type="xs:string" />
																<xs:element name="PrimerApellido"
																	type="xs:string" />
																<xs:element name="SegundoApellido"
																	type="xs:string" />
																<xs:element name="FechaDeNacimiento"
																	type="DateType" />
																<xs:element name="Edad"
																	type="xs:positiveInteger" />
																<xs:element name="Parentesco">
																	<xs:simpleType>
																		<xs:restriction
																			base="xs:string">
																			<xs:enumeration
																				value="Concubino" />
																			<xs:enumeration
																				value="Cónyugue" />
																			<xs:enumeration
																				value="Hija" />
																			<xs:enumeration
																				value="Hija mayor de edad" />
																			<xs:enumeration
																				value="Hijo" />
																			<xs:enumeration
																				value="Hijo mayor de edad" />
																			<xs:enumeration
																				value="Madre" />
																			<xs:enumeration
																				value="Padre" />
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
																<xs:element name="Sexo"
																	type="Sexo2Type" />
																<xs:element name="Orden">
																	<xs:simpleType>
																		<xs:restriction
																			base="xs:integer">
																			<xs:maxInclusive
																				value="999" />
																		</xs:restriction>
																	</xs:simpleType>
																</xs:element>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:choice>
												<xs:attribute name="Nacionalidad" use="required">
													<xs:simpleType>
														<xs:restriction base="xs:string">
															<xs:enumeration value="Mexican" />
															<xs:enumeration value="Foreign" />
														</xs:restriction>
													</xs:simpleType>
												</xs:attribute>
											</xs:complexType>
										</xs:element>
									</xs:sequence>

								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>

				<xs:element name="TrayectoriaAcademica">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Trayectoria" minOccurs="1" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="NivelDeEscolaridad" minOccurs="1"
											maxOccurs="unbounded">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="Doctorado" />
													<xs:enumeration value="Especialidad" />
													<xs:enumeration value="Licenciatura" />
													<xs:enumeration value="Maestría" />
													<xs:enumeration value="Preparatoria" />
													<xs:enumeration value="Técnico Superior" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>

										<xs:element name="Titulo" type="xs:string" minOccurs="1"
											maxOccurs="1" />

										<xs:element name="EstadoDelGrado" minOccurs="1"
											maxOccurs="1">
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:enumeration value="Créditos terminados" />
													<xs:enumeration value="Grado obtenido" />
													<xs:enumeration
														value="Título o grado en proceso" />
													<xs:enumeration value="Truncado" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>

										<xs:element name="Instituciones" minOccurs="1" maxOccurs="1">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Institucion"
														type="InstitucionType"
														minOccurs="1" maxOccurs="unbounded" />
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="Cursos" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Curso" type="CursoType"
											minOccurs="0"
											maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="Certificaciones" minOccurs="0"
								maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Certificacion"
											type="CertificacionType"
											minOccurs="0" maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>

							</xs:element>

							<xs:element name="Logros" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Logro" type="LogroType"
											minOccurs="0"
											maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="IdiomasLenguas" minOccurs="0"
								maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Idioma" type="IdiomaType"
											minOccurs="0"
											maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

						</xs:sequence>
					</xs:complexType>
				</xs:element>

				<xs:element name="TrayectoriaProfesional">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Empleos" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Empleo" type="EmpleoType" minOccurs="0"
											maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="Produccion">
								<xs:complexType>
									<xs:choice>
										<!-- Producción Humanista -->
										<xs:sequence>
											<xs:element name="Articulo" type="ArticuloType"
												minOccurs="0" maxOccurs="unbounded" />
											<xs:element name="Libro" type="LibroType" minOccurs="0"
												maxOccurs="unbounded" />
											<xs:element name="Capitulo" type="CapituloType"
												minOccurs="0" maxOccurs="unbounded" />
										</xs:sequence>

										<!-- Producción Tecnológica -->
										<xs:sequence>
											<xs:element name="Desarrollo" type="DesarrolloType"
												minOccurs="0" maxOccurs="unbounded" />
											<xs:element name="PropiedadIntelectual"
												type="PropiedadIntelectualType"
												minOccurs="0" maxOccurs="unbounded" />
											<xs:element name="TransferenciaTecnologica"
												type="TransferenciaType" minOccurs="0"
												maxOccurs="unbounded" />
										</xs:sequence>
									</xs:choice>

									<xs:attribute name="Tipo" use="required">
										<xs:simpleType>
											<xs:restriction base="xs:string">
												<xs:enumeration value="Humanista" />
												<xs:enumeration value="Tecnológica" />
											</xs:restriction>
										</xs:simpleType>
									</xs:attribute>
								</xs:complexType>

							</xs:element>

							<xs:element name="Evaluaciones" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Evaluacion" type="EvaluacionType"
											minOccurs="0" maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="Estancias" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Estancia" type="EstanciaType"
											minOccurs="0"
											maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="HCTI" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Comunidad" type="ComunidadHCTIType"
											minOccurs="0" maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="AccesoUniversalAlConocimiento" minOccurs="0"
								maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="Acceso"
											type="AccesoAlConocimientoType" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>

							<xs:element name="ENI" minOccurs="0" maxOccurs="1">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="ENI" type="ENI_GEMA_SisdaiType"
											minOccurs="0"
											maxOccurs="unbounded" />
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>

				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>

	<!--Complex
	Types-->

	<!--Semblanza-->
	<xs:simpleType name="SemblanzaType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="2000" />
		</xs:restriction>
	</xs:simpleType>

	<!--Contacto-->
	<xs:complexType name="ContactoType">
		<xs:sequence>
			<xs:element name="Correo" type="CorreoType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Movil" type="TelefonoType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Telefono" type="TelefonoType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="RedSocial" type="xs:string" minOccurs="0" maxOccurs="1" />
		</xs:sequence>
	</xs:complexType>

	<!--Correo-->
	<xs:simpleType name="CorreoContenido">
		<xs:restriction base="xs:string">
			<xs:pattern value="[^@]+@[^@]+\.[^@]+" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="CorreoType">
		<xs:simpleContent>
			<xs:extension base="CorreoContenido">
				<xs:attribute name="Principal" use="required" type="xs:boolean" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<xs:complexType name="CorreoContactoType">
		<xs:simpleContent>
			<xs:extension base="CorreoContenido">
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!--Número
	de teléfono-->
	<xs:simpleType name="DiezDigitos">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{10}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="TelefonoType">
		<xs:simpleContent>
			<xs:extension base="DiezDigitos">
				<xs:attribute name="Principal" type="xs:boolean" use="required" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!--Habilidad-->
	<xs:complexType name="HabilidadType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="experiencia" use="required">
					<xs:simpleType>
						<xs:restriction base="xs:integer">
							<xs:enumeration value="25" />
							<xs:enumeration value="50" />
							<xs:enumeration value="75" />
							<xs:enumeration value="100" />
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!--Identificadores
	de Autor-->
	<xs:complexType name="IdentificadorType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="organizacion" use="required">
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="ORC" />
							<xs:enumeration value="Thomson" />
							<xs:enumeration value="arXiv" />
							<xs:enumeration value="PubMed" />
							<xs:enumeration value="Open" />
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!--CURP-->
	<xs:simpleType name="CURPType">
		<xs:restriction base="xs:string">
			<xs:length value="18" />
		</xs:restriction>
	</xs:simpleType>

	<!--RFC-->
	<xs:simpleType name="RFCType">
		<xs:restriction base="xs:string">
			<xs:length value="13" />
		</xs:restriction>
	</xs:simpleType>

	<!--Fecha-->
	<xs:simpleType name="DateType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{4}-\d{2}-\d{2}" />
		</xs:restriction>
	</xs:simpleType>

	<!--Sexo-->
	<xs:simpleType name="SexoType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="masculino" />
			<xs:enumeration value="femenino" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="Sexo2Type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Masculino" />
			<xs:enumeration value="Femenino" />
			<xs:enumeration value="No binario" />
			<xs:enumeration value="Otro" />
		</xs:restriction>
	</xs:simpleType>

	<!--Estado
	Civil-->
	<xs:simpleType name="EstadoCivilType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Casado(a)" />
			<xs:enumeration value="Contrato de convivencia" />
			<xs:enumeration value="Divorciado(a)" />
			<xs:enumeration value="Separado(a)" />
			<xs:enumeration value="Soltero(a)" />
			<xs:enumeration value="Unión libre" />
			<xs:enumeration value="Viudo(a)" />
		</xs:restriction>
	</xs:simpleType>

	<!--Código
	postal-->
	<xs:simpleType name="CodigoPostalType">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{1,5}" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="CPMin5Max10Type">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{5,10}" />
		</xs:restriction>
	</xs:simpleType>

	<!--Tipo
	Domicilio-->
	<xs:simpleType name="TipoDomicilioType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Urbano" />
			<xs:enumeration value="Rural" />
		</xs:restriction>
	</xs:simpleType>

	<!--Descripción-->
	<xs:simpleType name="Descripcion500Type">
		<xs:restriction base="xs:string">
			<xs:maxLength value="500" />
		</xs:restriction>
	</xs:simpleType>

	<!--Número
	Exterior-->
	<xs:complexType name="NumeroExteriorType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="ParteNumerica" type="xs:string" use="optional" />
				<xs:attribute name="NumeroExteriorAnterior" type="xs:string" use="optional" />
				<xs:attribute name="ParteAlfanumerica" type="xs:string" use="optional" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!--Numéro
	Interior-->
	<xs:complexType name="NumeroInteriorType">
		<xs:simpleContent>
			<xs:extension base="xs:string">
				<xs:attribute name="ParteNumerica" type="xs:string" use="optional" />
				<xs:attribute name="ParteAlfanumerica" type="xs:string" use="optional" />
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>

	<!--Mínimo
	2 caracteres-->
	<xs:simpleType name="NombreMin2Type">
		<xs:restriction base="xs:string">
			<xs:minLength value="2" />
		</xs:restriction>
	</xs:simpleType>

	<!--Institución-->
	<xs:complexType name="InstitucionType">
		<xs:choice>
			<!-- Institución Nacional -->
			<xs:element name="Nacional">
				<xs:complexType>
					<xs:choice>
						<!-- Pública-Federal o Privada -->
						<xs:element name="SectorSimple">
							<xs:complexType>
								<xs:attribute name="Sector" use="required">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="Pública-Federal" />
											<xs:enumeration value="Privada" />
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:complexType>
						</xs:element>

						<!-- Pública-Estatal -->
						<xs:element name="SectorEstatal">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Entidad" type="xs:string" />
								</xs:sequence>
								<xs:attribute name="Sector" use="required" fixed="Pública-Estatal" />
							</xs:complexType>
						</xs:element>

						<!-- Pública-Municipal -->
						<xs:element name="SectorMunicipal">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Entidad" type="xs:string" />
									<xs:element name="MunicipioAlcaldia" type="xs:string" />
								</xs:sequence>
								<xs:attribute name="Sector" use="required" fixed="Pública-Municipal" />
							</xs:complexType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>

			<!-- Institución Extranjera -->
			<xs:element name="Extranjera">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Pais" type="xs:string" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>

		<xs:attribute name="TipoDeInstitucion" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Nacional" />
					<xs:enumeration value="Extranjera" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<!--Cursos-->
	<xs:complexType name="CursoType">
		<xs:sequence>
			<xs:element name="TipoDeFormacionContinua" type="xs:string" />
			<xs:element name="NombreDelCurso" type="xs:string" />
			<xs:element name="Anio" type="AnioType" />
			<xs:element name="HorasTotales" type="Horas4DigitosType" />
			<xs:element name="Institucion" type="InstitucionType" />
		</xs:sequence>
	</xs:complexType>

	<!--Menor
	a año N-->
	<xs:simpleType name="AnioType">
		<xs:restriction base="xs:gYear">
			<xs:maxInclusive value="2025" />
		</xs:restriction>
	</xs:simpleType>

	<!--Entre
	0 y 9999-->
	<xs:simpleType name="Horas4DigitosType">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="9999" />
		</xs:restriction>
	</xs:simpleType>

	<!--Certificaciones-->
	<xs:complexType name="CertificacionType">
		<xs:sequence>
			<xs:element name="NumeroDeFolio" type="Folio50Type" />
			<xs:element name="ConsejoOtorgado" type="xs:string" />
			<xs:element name="NombreDeLaCertificacion" type="xs:string" />
			<xs:element name="Especialidad" type="xs:string" />
			<xs:element name="Vigencia" type="VigenciaType" />
		</xs:sequence>
		<xs:attribute name="Tipo" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Certificacion" />
					<xs:enumeration value="Recertificacion" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<!--50
	caracteres alfanuméricos-->
	<xs:simpleType name="Folio50Type">
		<xs:restriction base="xs:string">
			<xs:maxLength value="50" />
		</xs:restriction>
	</xs:simpleType>

	<!--Vigencias-->
	<xs:complexType name="VigenciaType">
		<xs:attribute name="Inicio" type="DateType" use="required" />
		<xs:attribute name="Fin" type="DateType" use="required" />
	</xs:complexType>

	<!--Logros-->
	<xs:complexType name="LogroType">
		<xs:choice>
			<xs:element name="SECIHTI">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="NombreDistincion" type="xs:string" />
						<xs:element name="Anio" type="AnioType" />
					</xs:sequence>
				</xs:complexType>
			</xs:element>

			<xs:element name="Otros">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Anio" type="AnioType" />
						<xs:element name="Nombre" type="xs:string" />
						<xs:element name="Institución" type="InstitucionType" />
						<xs:element name="Pais" type="xs:string" />
						<xs:element name="Descripcion" type="DescripcionType" />

					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:choice>

		<xs:attribute name="Tipo" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="SECIHTI" />
					<xs:enumeration value="Otros" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<!--500
	caracteres-->
	<xs:simpleType name="DescripcionType">
		<xs:restriction base="xs:string">
			<xs:maxLength value="500"></xs:maxLength>
		</xs:restriction>
	</xs:simpleType>

	<!--Idiomas
	o lenguas-->
	<xs:complexType name="IdiomaType">
		<xs:sequence>
			<xs:element name="Idioma" type="xs:string" />
			<xs:element name="GradoDeDominio" type="xs:string" />
			<xs:element name="NivelDeConversacion" type="xs:string" />
			<xs:element name="NivelDeLectura" type="xs:string" />
			<xs:element name="NivelDeEscritura" type="xs:string" />

			<xs:element name="Certificacion" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Institucion" type="xs:string" minOccurs="0" />
						<xs:element name="FechaDeEvaluacion" type="DateType" />
						<xs:element name="Documento" type="xs:string" />
						<xs:element name="Vigencia" type="VigenciaType" />
						<xs:element name="Puntos" type="xs:string" />
						<xs:element name="Nivel" type="xs:string" />
					</xs:sequence>
					<xs:attribute name="Certificacion" use="optional" fixed="True" />
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Certificacion" use="optional" fixed="False" />
	</xs:complexType>

	<!--Empleos-->
	<xs:complexType name="EmpleoType">
		<xs:sequence>
			<xs:element name="Institucion" type="InstitucionType" />
			<xs:element name="FechaInicio" type="xs:date" />
			<xs:element name="NombreDelPuestoONombramiento" type="Texto1000Type" />
			<xs:element name="Logros" type="Texto1000Type" minOccurs="0" />

			<xs:element name="FechaFin" type="xs:date" minOccurs="0" />
		</xs:sequence>
		<xs:attribute name="EmpleoActual" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:boolean" />
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="EmpleoPrincipal" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:boolean" />
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="FechaFinalizacion" use="optional">
			<xs:simpleType>
				<xs:restriction base="xs:boolean" />
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>

	<!--1000
	caracteres-->
	<xs:simpleType name="Texto1000Type">
		<xs:restriction base="xs:string">
			<xs:maxLength value="1000" />
		</xs:restriction>
	</xs:simpleType>

	<!--Artículos-->
	<xs:complexType name="ArticuloType">
		<xs:sequence>

			<!-- ISNN -->
			<xs:choice>
				<xs:sequence>
					<xs:element name="ISNNImpreso" type="Folio50Type" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="ISNNElectronico" type="Folio50Type" />
				</xs:sequence>
				<xs:sequence>
					<xs:element name="ISNNImpreso" type="Folio50Type" />
					<xs:element name="ISNNElectronico" type="Folio50Type" />
				</xs:sequence>
			</xs:choice>

			<!-- Campos principales -->
			<xs:element name="DOI" type="AlfaNum70Type" />
			<xs:element name="NombreDeLaRevista" type="xs:string" />
			<xs:element name="TituloDeArticulo" type="xs:string" />
			<xs:element name="AnioDeLaPublicacion" type="AnioType" />

			<!-- Apoyo SECIHTI -->
			<xs:element name="Programa" type="xs:string" minOccurs="0" />

			<xs:element name="RolDeParticipacion" type="xs:string" />
			<xs:element name="EstatusDePublicacion" type="xs:string" />
			<xs:element name="Objetivo" type="xs:string" />

			<xs:element name="Citas" type="CitasType" minOccurs="0" maxOccurs="1" />

			<xs:element name="Autores" type="AutoresType" />

			<xs:element name="EjeSECIHTI" type="xs:string" />

		</xs:sequence>

		<xs:attribute name="ISNN" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Impreso" />
					<xs:enumeration value="Electrónico" />
					<xs:enumeration value="Ambos" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>

		<xs:attribute name="ApoyoSECIHTI" use="required" type="xs:boolean" />
	</xs:complexType>

	<!--70
	Caracteres-->
	<xs:simpleType name="AlfaNum70Type">
		<xs:restriction base="xs:string">
			<xs:maxLength value="70" />
		</xs:restriction>
	</xs:simpleType>

	<!--Citas-->
	<xs:complexType name="CitasType">
		<xs:sequence>
			<xs:element name="URLCita" type="xs:string" />
			<xs:element name="CitaA" type="xs:integer" />
			<xs:element name="CitaB" type="xs:integer" />
			<xs:element name="TotalDeCitas" type="xs:integer" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="CitasLibroType">
		<xs:sequence>
			<xs:element name="URLCita" type="xs:string" />
			<xs:element name="CitaA" type="xs:integer" />
			<xs:element name="CitaB" type="xs:integer" />
			<xs:element name="TotalDeCitas" type="xs:integer" />
			<xs:element name="EjeSECIHTI" type="xs:string" />
		</xs:sequence>
	</xs:complexType>


	<!--Autores-->
	<xs:complexType name="AutoresType">
		<xs:sequence>
			<xs:element name="Autor" type="AutorType" minOccurs="1" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="AutorType">
		<xs:sequence>
			<xs:element name="Nombre" type="xs:string" />
			<xs:element name="PrimerApellido" type="xs:string" />
			<xs:element name="SegundoApellido" type="xs:string" minOccurs="0" />
			<xs:element name="ORCID" type="xs:string" />
		</xs:sequence>
		<xs:attribute name="Orden" use="required" type="xs:integer" />
	</xs:complexType>

	<!--Libros-->
	<xs:complexType name="LibroType">
		<xs:sequence>
			<xs:element name="ISBN" type="Folio50Type" />
			<xs:element name="DOI" type="AlfaNum70Type" />
			<xs:element name="Titulo" type="xs:string" />
			<xs:element name="AnioDeLaPublicacion" type="AnioType" />
			<xs:element name="Editorial" type="xs:string" />
			<xs:element name="Pais" type="xs:string" />
			<xs:element name="Idioma" type="xs:string" />
			<xs:element name="ISBNTraducido" type="Folio50Type" />
			<xs:element name="IdiomaTraducido" type="xs:string" />
			<xs:element name="TituloTraducido" type="xs:string" />

			<!-- Programa solo si ApoyoSECIHTI=True, validación fuera del XSD -->
			<xs:element name="Programa" type="xs:string" minOccurs="0" />

