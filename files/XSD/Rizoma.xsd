	GEMA Sisdai-->
	<xs:complexType name="ENI_GEMA_SisdaiType">
		<xs:sequence>
			<xs:element name="ENI" type="ENIType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="GEMA" type="GEMAType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Sisdai" type="SisdaiType" minOccurs="0" maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

