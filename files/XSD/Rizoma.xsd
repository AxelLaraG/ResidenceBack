			<xs:element name="Rol" type="xs:string" />
		</xs:sequence>
	</xs:complexType>

	<!--4000
	caracteres-->
	<xs:simpleType name="Texto4000Type">
		<xs:restriction base="xs:string">
			<xs:maxLength value="4000" />
		</xs:restriction>
	</xs:simpleType>

	<!--Máximo
	10 dígitos-->
	<xs:simpleType name="Horas10DigitosType">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1" />
			<xs:maxInclusive value="9999999999" />
		</xs:restriction>
	</xs:simpleType>

