		</xs:sequence>
	</xs:complexType>

	<!--Autores
	de Trabajos-->
	<xs:complexType name="AutoresTrabajoType">
		<xs:sequence>
			<xs:element name="Nombre" type="xs:string" />
			<xs:element name="PrimerApellido" type="xs:string" />
