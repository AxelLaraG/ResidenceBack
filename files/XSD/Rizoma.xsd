	Universal al conocimiento-->
	<xs:complexType name="AccesoAlConocimientoType">
		<xs:sequence>
			<xs:element name="Difusion" type="DifusionType" minOccurs="0" maxOccurs="unbounded" />
			<xs:element name="Divulgacion" type="DivulgacionType" minOccurs="0"
				maxOccurs="unbounded" />
			<xs:element name="Participacion" type="ParticipacionType" minOccurs="0"
				maxOccurs="unbounded" />
